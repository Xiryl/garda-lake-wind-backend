{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": 0,
      "consistent-return": 0,
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off"
    }
  }
